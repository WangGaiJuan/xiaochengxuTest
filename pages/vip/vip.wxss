/* pages/vip/vip.wxss */
page {
  background-color: rgb(239, 239, 244);
}
.content{
  padding: 15rpx;
  min-height: 100%;
}

.vip{
  margin:auto;  
  text-align: center;
  background-color: #fff;
  display: flex;
  flex-direction: row;
  
}
.vip image {
  width:100%;
  margin-top: 10rpx;
  margin-bottom: 20rpx;
}

.table {
  background: #fff;
  font-size: 28rpx;
}
.tr {
  display: flex;
  width: 100%;
  justify-content: center;
  height: 72rpx;
  align-items: center;
}
.td {
    width:40%;
    justify-content: center;
    text-align: center;
}

.bg-g{
  background: #F0F0F0;
}
.th {
  width: 40%;
  justify-content: center;
  background: #B3B3B3;
  color: #fff;
  display: flex;
  height: 72rpx;
  line-height: 72rpx;
  align-items: center;
}

.phone {
  color: #1A8AFA;
}

.none {
  width: 100%;
  height: 100%;
  text-align: center;
  padding-top:25%;
  padding-bottom: 50%;
  background: #fff;
}


